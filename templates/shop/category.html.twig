{% extends 'base.html.twig' %}

{% block title %}Start{% endblock %}

{% block body %}
    <h1>Start</h1>

    {% for article in articles %}
        <div style="margin: 35px">
            <h3>
                <a href="{{ path('article', {'id': article.id}) }}">{{ article.title }}</a>
            </h3>
            <p>{{ article.shortdesc }}</p>
        </div>
    {% endfor %}
{% endblock %}
